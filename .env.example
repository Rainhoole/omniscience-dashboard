# App
NODE_ENV=development
PORT=3200
API_SECRET=change-me-to-a-long-random-string
ADMIN_PASSWORD=change-me

# Postgres (docker-compose default)
DATABASE_URL=postgresql://omniscience:omniscience@postgres:5432/omniscience

# OpenClaw adapter token (required for /api/openclaw/events and /api/openclaw/ops)
OPENCLAW_ADAPTER_TOKEN=change-me-openclaw-token

# Optional: true external retry dispatch endpoint
# If unset, retry uses local fallback chain for observability continuity.
OPENCLAW_RETRY_DISPATCH_URL=
OPENCLAW_RETRY_DISPATCH_TOKEN=
